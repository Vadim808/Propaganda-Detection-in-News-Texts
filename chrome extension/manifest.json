	"manifest_version": 3,
	"name": "Text highlight",
	"permissions": ["scripting", "tabs",  "http://*/"],
	"host_permissions": ["http://127.0.0.1:5000/"],
	"version": "2.0",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
	//			"require.js",
				"content.js"
			]	
		}
	]
}